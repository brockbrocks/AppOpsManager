<resources>

    <style name="Theme.AppOpsManager" parent="Theme.Material3.DayNight">
        <item name="colorPrimary">@color/colorPrimary</item>
        <!-- <item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <item name="colorOnPrimary">@color/colorOnPrimary</item>

        <item name="colorAccent">@color/colorAccent</item>

        <!--consume-->
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowLightStatusBar">?attr/isLightTheme</item>
        <item name="android:colorBackground">@color/colorBackground</item>
        <item name="actionBarStyle">@style/Theme.AppOpsManager.actionBarStyle</item>
        <item name="materialCardViewStyle">@style/Theme.AppOpsManager.materialCardViewStyle</item>
    </style>

    <style name="Theme.AppOpsManager.actionBarStyle" parent="@style/Widget.Material3.Light.ActionBar.Solid">
        <item name="background">@null</item>
        <item name="elevation">0dp</item>
    </style>

    <style name="Theme.AppOpsManager.materialCardViewStyle" parent="@style/Widget.Material3.CardView.Filled">
        <item name="cardBackgroundColor">@color/cardBackgroundColor</item>
    </style>

    <style name="Theme.AppOpsManager.Starting" parent="Theme.SplashScreen">
        <item name="postSplashScreenTheme">@style/Theme.AppOpsManager</item>
    </style>


</resources>